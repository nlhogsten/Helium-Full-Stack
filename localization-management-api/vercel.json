{
  "version": 2,
  "builds": [
    {
      "src": "src/localization_management_api/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/localization_management_api/main.py"
    }
  ]
}
